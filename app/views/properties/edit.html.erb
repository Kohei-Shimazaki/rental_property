<h2>物件編集</h2>
<%= form_with(model: @property, local: true) do |form| %>
  <%= form.label :name, "物件名" %><br>
  <%= form.text_field :name %><br>
  <%= form.label :rent, "賃料" %><br>
  <%= form.number_field :rent %>円<br>
  <%= form.label :address, "住所" %><br>
  <%= form.text_field :address %><br>
  <%= form.label :age, "築年数" %><br>
  <%= form.number_field :age %>年<br>
  <%= form.label :note, "備考" %><br>
  <%= form.text_area :note %><br>
  <%= form.submit "登録する" %><br>
<% end %>
<%= link_to "詳細", property_path(@property.id) %> | <%= link_to "戻る", properties_path %>
